  function select_seller_dialog($form_to_submit, $product_id, $reload_page){
    reload_page = true
    $.ajax({
        url: "/pub/api/products/get_seller_stocks.json?variant_id=" + $product_id,
        dataType: "json",
        type: 'GET',
        async: false,
        success: function(data) {
          var html_data='';
          if (data.success == true)
          {
            var dialogOptions = {
              autoOpen: false,
              modal: true,
              height: "auto",
              width: "auto",
              dialogClass: "select-seller-dialog-wrapper",
              title: $("#select_seller_popup_markup .title:first").text(),
              close: function(){
                pop_up_close_handler(reload_page);
              },
              buttons:[{
                text: $("#golden_params_popup_markup .cancel-btn-text:first").text(),
                class: "medium-prominent-btn",
                click: function() {
                  $(this).dialog("close");
                }
              },
              {
                text: $("#golden_params_popup_markup .proceed-btn-text:first").text(),
                click: function(data){
                  var dialog_bttn = $(this);
                  var selected_val = dialog_bttn.closest("#select_seller_dialog").find("input[name='source_id']:checked").val();
                  if(selected_val != undefined){
                    if($("#is-pdp").length > 0)
                    {
                      var form_id = $form_to_submit.closest('form').attr('id')
                      $('body').find("#"+form_id).each(function(){
                        var _cart_form = $(this);
                        _cart_form.find("#stock_source").val(selected_val);
                      })
                    }
                    else
                    {
                      $form_to_submit.find("#stock_source").val(selected_val);
                    }
                    window.BID_REF_PRICE_SELECTION_STOCK_CHANGED = true
                    $form_to_submit.submit();
                    reload_page = false
                    dialog_bttn.dialog("close");
                  }
                  else{
                    alert("Cannot proceed without selecting any seller")
                  }
                }
              }]
            }

            $("#select_seller_popup_markup .header .text").text(data.product_title)
            header_msg_html = $("#select_seller_popup_markup .header").html()
            seller_table = $('#select_seller_popup_table_markup').html()
            seller_popup_html = "<div id='select_seller_dialog'>" + header_msg_html + seller_table + "</div>"
            html_data= Mustache.render(seller_popup_html, data);
            $(html_data).dialog(dialogOptions).dialog("open");
            $("#select_seller_dialog").find("input[name='source_id']:first").attr("checked",true);
          }
          else{
            html_data = $("#select_seller_popup_markup .error").html()
            $(html_data).dialog().dialog("open");
          }
        }
      });
  }

  function pop_up_close_handler(reload_status){
    if (reload_status == true) {
      if(/^(\/\w{2})?\/products\/compare/.test(window.location.pathname)){
        BAB.Cart.disable_add_to_cart_buttons_in_compare_page();
        window.location.search += '&force_compute=true';
      } 
      else{
        window.location.href = window.location.href;
      }
    }
  }


  $(".add-to-cart-fake-action-button").live("click", function(e) {
    $form_to_submit = $(this).parent();
    $bnid = $(this).data("bnid");
    $product_id = $(this).data("product_id");
    $source_id = $(this).data("source_id");
    $checksum = $(this).data("checksum");
    $iscompare = $(this).data("iscompare");
    $reload_page = true
    $do_not_proceed = false

    url = "/pub/api/browse_nodes/applicable_checkout_options.json?variant_id=" + $product_id + "&source_id=" + $source_id + "&is_compare=" + $iscompare + "&checksum=" + $checksum
    dp_applicable = true

    $.ajax({
      url: url,
      dataType: "json",
      type: 'GET',
      async: false,
      success: function(data) {
        var force_bid_checkout = data.force_bid_checkout || false
        dp_applicable = (data.options.length == 0 || ((data.options.indexOf("Direct Buy") != -1) || (data.options.indexOf("Soft Bid") != -1)))
        var force_soft_bid_checkout = data.intents.indexOf("soft_bid") != -1
        if (force_soft_bid_checkout || force_bid_checkout || (dp_applicable && data.options.length == 1)){
          $do_not_proceed = true
          $form_to_submit.submit();
        }

        // Build the Iframe dialog
        $("#filters_selection_iframe_dialogue").dialog({
          autoOpen: false,
          modal: true,
          height: "auto",
          width: "auto",
          title: $('#filters_selection_popup_markup .title:first').text(),
          close: function(){
            pop_up_close_handler($reload_page);
          },
          open: function() {
            $(this).parent().find("span.ui-dialog-title").append("<span id='filters_selection_iframe_icon'></span>");

            var qtip_html = $("#filters_selection_popup_markup .qtip_html").html()

            $('#filters_selection_iframe_icon').qtip({
              content:{
                title: {text: '.', button: true },
                text:qtip_html
              },
              position:{
                my:'top center',
                at:'bottom center'
              },
              style:{
                classes:'filters_selection_iframe_qtip',
                width:550,
                tip: {
                  width: 15
                }
              },
              show: 'click',
              hide: 'click'
            });

            $('#filters_selection_iframe_icon').trigger("click");
            $("#facets #product > h6").trigger("click");
          },
          buttons: [{
            text: $("#golden_params_popup_markup .proceed-btn-text:first").text(),
            click: function() {
              dialog_bttn = $(this);
              $.ajax({
                url: "/pub/api/browse_nodes/validate_buyer_selections.json?bnid=" + $bnid,
                dataType: "json",
                type: 'GET',
                success: function(data) {
                  if (data.success == true)
                  {
                    $reload_page = false
                    select_seller_dialog($form_to_submit, $product_id, $reload_page);
                    dialog_bttn.dialog("close");
                  }
                  else
                  {
                    filters_data = data.filters_data.sort(function(a, b){
                      var aVal = (a[1] == "selected")?0:1
                      var bVal = (b[1] == "selected")?0:1
                      return aVal - bVal
                    })

                    var filters_data_html = ""
                    for(i in filters_data){
                      filters_data_html += "<span class='filters-status-element " + filters_data[i][1] + "'>" + filters_data[i][0] + "</span>"
                    }
                    $('#filters_selection_popup_markup .status_html .filters-status').html(filters_data_html)
                    $("#filters-selection-status").html($('#filters_selection_popup_markup .status_html'))
                    $('#filters_selection_popup_markup .overview .count').text(data.selection_count_message)
                    $("#filters-selection-overview").text($('#filters_selection_popup_markup .overview:first').text())
                    incomplete_golden_filters_dialog($bnid,$product_id)
                    $("#all_filters_not_selected_dialogue").dialog("open")
                  }
                },
                error: function(data) {
                  alert("Cannot proceed at the moment. Please try again.")
                }
              });
            }
          }]
        });

        if($do_not_proceed == true){
          return true;
        }

        if(dp_applicable == true){
          // First open the intent of buy dialog
          buyer_intent_dialog($bnid,$product_id,$form_to_submit,data)
          $("#intent_of_buying_dialogue").dialog("open")
        }
        else{
          if(set_bid_intent()){
            iframe_url = filters_iframe_url($bnid,$product_id)
            $("#filters_selection_iframe").attr("src", iframe_url);
            $("#filters_selection_iframe_dialogue").dialog("open")
          }
        }
      },
      error: function(data) {
        alert("Cannot proceed at the moment. Please try again.")
      }
    });

    if($(this).data("origin_country_buyer_advisory") == 'true'){
      alert('<%= CONFIG.helptext.stock.origin_country_buyer_advisory %>')
    }

  });

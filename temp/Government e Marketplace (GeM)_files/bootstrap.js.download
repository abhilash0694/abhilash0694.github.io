        productHistory: {
            MAX_SIZE : 10,
            load : function(){
                var pH = $.cookies.get("prod_hist"), _productHistory = [];
                if( $.isArray(pH) ){
                    _productHistory = pH.slice(-BAB.productHistory.MAX_SIZE);
                }
                // BAB.productHistory.load = function(){
                //     return _productHistory;
                // };
                return _productHistory;
            },
            add: function(variantId){
                pH = BAB.productHistory.load();

                index = $.inArray(variantId, pH);
                histLength = pH.length
                if( histLength && index !== -1 && index != (histLength -1)){
                    pH.splice(index,1);
                }
                if( !pH.length || index != (histLength -1)){
                    pH.push(variantId);
                }
                var expiry = new Date();
                expiry.setTime(expiry.getTime() + 30 * 24 * 60 * 60 * 1E3);
                $.cookies.set('prod_hist', pH, {'expiresAt': expiry});
            },
            updateWidget: function(force){
                pH = BAB.productHistory.load();
                if(!pH.length){
                    $(".product-history").closest('.product-history-widget').addClass('hide');
                    return;
                }
                var activeProductHistoryWidget = $(".product-history").not('.initialized');
                activeProductHistoryWidget.each(function(){
                    var $me = $(this);
                    if(!($me.is(':visible') || force)){
                      return;
                    }
                    var numHistory = $me.data('num-size');
                    var cacheKey = $me.data('widget-cache-key');
                    var showAll = $me.data('show-all');
                    var variantIds = pH.slice(-numHistory);
                    $me.addClass('initialized');
                    var loader = $('<div>',{'class': 'prod_hist_loader'});
                    $me.append(loader);
                    numHistory && $.ajax({
                        url : "/products/variant_details",
                        data : {
                            variant_ids : variantIds,
                            cache_key: cacheKey,
                            render_options: JSON.stringify($me.data('render-options'))
                        },
                        success : function(data){
                          if(data && data.html){
                            var variantHtml;
                            for(var i= (variantIds.length-1) ; i >= 0; i--){
                              if(( variantHtml = data.html[variantIds[i]] ) &&
                                  (showAll || !$(variantHtml).hasClass('btn-out_of_stock')) ){
                                var item = $me.find('.item-list .empty-item:first');
                                item.html(variantHtml);
                                item.removeClass('hide empty-item');
                              }
                            }
                            BAB.initProducts();
                            $(document).trigger('widget.product-history.loaded',$me);
                          }
                        }
                    })
                    .done(function(){
                      $me.find('.prod_hist_loader').remove();
                    });
                });
            }
        }
    };

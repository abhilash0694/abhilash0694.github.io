  var withCoCombination = function(catalogOptionIds, optToEnabledOtherOpts, callback,  selection, level) {
    if(!catalogOptionIds.length || level >= catalogOptionIds.length)
      return;

    selection = selection || [];
    level = level || 0;

    var parentCo = selection[selection.length - 1];
    var validCatalogOptions = optToEnabledOtherOpts[parentCo] || [];

    if(level < catalogOptionIds.length){

      $.each(catalogOptionIds[level], function(index, catalogOptionId){

        if( !parentCo || validCatalogOptions.indexOf(""+catalogOptionId) !== -1){
          var currentSelection = selection.slice();
          currentSelection.push(catalogOptionId);

          // recursively compute the path using opt to enabled other path as filter
          withCoCombination(catalogOptionIds, optToEnabledOtherOpts, callback, currentSelection, level+1);

          if(level === catalogOptionIds.length -1){
            callback(currentSelection);
          }
        }
      });
    }
  };

  var traverseTree = function(name, node, filter, callback){
    if(!node)
      return false;

    if(node.leaf){
      if(filter(name, node)){
        callback(name, node);
        return true;
      }
      else{
        return false;
      }
    }
    else{
      var propagate = true;
      $.each(node, function(name, child){
        propagate = traverseTree(name, child, filter, callback) && propagate;
      });
      if(propagate){
        callback(name, node);
        return true;
      }
      else{
        return false;
      }
    }
  };

  RenderVariant.updateStatusClasses = function ($wrapper, variantId, catalogId, options) {
    var $catalogOptionsWrapper = $wrapper.find('.catalog-options');
    var $catalogOptions = $catalogOptionsWrapper.find(".cat-options");

    var treeCache= RenderVariant.data.tree || {};
    treeCache = treeCache[catalogId];
    if(!treeCache){
      treeCache = {};
      var outOfStock = options.variant_data.variant_properties || {};
      outOfStock = outOfStock[catalogId] || {};
      var catalogOptionIds = $catalogOptions.map(function () {
        var catalogOptionType = $(this),
          catalogOptions = catalogOptionType.find('.cat-option');
      return catalogOptions.map(function () {
        return $(this).data('co-id');
      });
      });
      var optToEnabledOtherOpts = options.variant_data.opt_to_enabled_other_opts || {};
      optToEnabledOtherOpts = optToEnabledOtherOpts[catalogId] || {};
      var variantToOpts = options.variant_data.variant_to_opts || {};
      variantToOpts = variantToOpts[catalogId] || {};
      var optsToVariant = {};
      $.each(variantToOpts, function (k, v) {
        optsToVariant[v] = k;
      });
      var updateTreeCache  = function (coCombination) {
        var coCombinationStr = coCombination.join(',');
        var node =  treeCache;
        $.each(coCombination, function(i, co){
          node[co] = node[co] || {};
          node = node[co];
        });
        node.leaf = true;
        var variant = optsToVariant[coCombinationStr];
        if (variant) {
          var oos = outOfStock[variant] || {};
          oos = oos.out_of_stock;
          if (oos) {
            node.oos = true;
          }
        }
      };
      withCoCombination(catalogOptionIds, optToEnabledOtherOpts, updateTreeCache);

      var rootNode = {tree:{}};
      rootNode.tree[catalogId] = treeCache;
      RenderVariant.updateDataCache(rootNode)
    }

    var filter = function(name, node){
      return node.oos == true;
    }
    var callback = function(name, node){
      $catalogOptionsWrapper.find('#co-' + name).addClass('out-of-stock-co');
    }

    if (!$catalogOptionsWrapper.hasClass('cops-init')) {
      $.each(treeCache, function(name , node){
        traverseTree(name, node, filter, callback)
      });
    }

    // Clear status classes for all except first level
    $catalogOptionsWrapper.find('.cat-options:gt(0)').find('.cat-option').each(function(){
      RenderVariant.removeClass($(this), "out-of-stock-co");
    });

    var selectedCoId = $catalogOptions.first().find('.cat-option.selected').data('co-id');
    traverseTree(selectedCoId, treeCache[selectedCoId], filter, callback);

    // when all variants are out of stock hide catalog options
    if (!$catalogOptionsWrapper.hasClass('cops-init')) {
      if($catalogOptions.find('.cat-option').length === $catalogOptions.find('.cat-option.out-of-stock-co').length){
        $catalogOptionsWrapper.addClass("out-of-stock-co") ;
      }
    }
  }


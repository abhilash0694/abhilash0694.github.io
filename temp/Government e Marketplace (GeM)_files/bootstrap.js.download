          "variant_to_opts" : options.variant_to_opts,
          "opt_to_enabled_other_opts" : options.opt_to_enabled_other_opts,
          "opt_type_to_ops" : options.opt_type_to_ops,
          "variant_to_default_image_id" : options.variant_to_default_image_id,
          "variant_properties" : options.variant_properties
        },
        "template":{
          'data': options.variant_sections,
          "method": "id"
        },
        'wrapper': '#product-detail-page',
        'onVariantChanged': function(variantData) {
          var variantId = variantData.variantId
          var catalogId = variantData.catalogId;
          $(document).trigger('variant_changed', [variantId, options.variant_properties[catalogId][variantId]]);
        }
      });
    ```
*/

+function($, global) {
  var RenderVariant = {};
  RenderVariant.DEFAULT_RENDER_VARIANT_OPTIONS = {
    'wrapper'          : '.variant-wrapper',
    'variant'          : null,
    'onVariantChanged' : $.noop
  };

  RenderVariant.init = function($cops, options) {
    if (!$cops.hasClass('cops-init')) {
      var $wrapper      = $cops.closest(options.wrapper);
      this.$element = $cops;
      this.options  = options || {};
      var variantId = options.variant || this.$element.data('var-id'),
        catalogId   = this.$element.data('cat-id');
      if (variantId && catalogId) {
        // Add disabled class for catalog option
        var variantToOpts = RenderVariant.data.variant_to_opts || {};
        variantToOpts = variantToOpts[catalogId] || {};
        coCombination = (variantToOpts[variantId] || "").split(',');
        var $catalogOptions = $cops.find('.cat-options');
        if(coCombination && (coCombination.length == $catalogOptions.length)){
          $catalogOptions.each(function(i, coType){
            coId = coCombination[i];
            RenderVariant.selectCatalogOption($cops, $catalogOptions, $(coType), catalogId, coId);
          });
        }
        this.selectVariant($wrapper, variantId, catalogId, this.options);
        this.addEventListeners();
      }
      $cops.addClass('cops-init');
    }
  };
  RenderVariant.addEventListeners = function() {
    var dropdown = this.$element.find('.cat-options').is('select');
    if(dropdown){
      this.$element.on("change.renderVariant", ".cat-options", this.onCatalogOptionChange(this.options, dropdown));
    }
    else{
      this.$element.on("click.renderVariant", ".cat-option", this.onCatalogOptionChange(this.options, dropdown));
    }
  };
  RenderVariant.data = {};
  RenderVariant.updateDataCache = function(data) {
    $.extend(true, this.data, data);
  };
  RenderVariant.removeEventListeners = function() {
    this.$element.off("click.renderVariant");
  };

  RenderVariant.onCatalogOptionChange = function(opts, dropdown) {
    var options = $.extend({}, RenderVariant.DEFAULT_RENDER_VARIANT_OPTIONS, opts || {});

    return function(event) {
      event.preventDefault();
      var $selectedCatalogOption    = (dropdown? $(this).find(':selected') : $(this)),
        $catalogOptionsWrapper      = $selectedCatalogOption.closest('.catalog-options'),
        catalogId                   = $.trim($catalogOptionsWrapper.data("cat-id")),
        $selectedCatalogOptionsType = $selectedCatalogOption.closest('.cat-options'),
        $wrapper                    = $selectedCatalogOptionsType.closest(options.wrapper),
        coId                        = parseInt($selectedCatalogOption.data("co-id"), 10),
        coType                      = $selectedCatalogOption.data("co-type"),
        $catalogOptions             = $catalogOptionsWrapper.find('.cat-options'),
        coCombination               = [],
        variantId                   = '';

      if(!($selectedCatalogOption.hasClass("selected"))) {
        RenderVariant.clearCatalogOptionsSelection($catalogOptions);
        coCombination = RenderVariant.findCatalogOptionCombination($catalogOptionsWrapper, $catalogOptions, $selectedCatalogOptionsType, $selectedCatalogOption, catalogId, coId, coType);
        variantId = RenderVariant.findVariant($catalogOptionsWrapper, $catalogOptions, catalogId, coId, coCombination);
        if (variantId) {
          RenderVariant.selectVariant($wrapper, variantId, catalogId, options);
        }
      }
      return false;
    };
  };

  RenderVariant.clearCatalogOptionsSelection = function($catalogOptions) {
    $catalogOptions.data({
      mark_selected: false
    });
  };

  RenderVariant.findCatalogOptionCombination = function($catalogOptionsWrapper, $catalogOptions, $selectedCatalogOptionsType, $selectedCatalogOption, catalogId, coId, coType) {
    var coCombination = [],
      $otherCatalogOptionsType = $catalogOptionsWrapper.find(".cat-options[data-type!='" + coType + "']"),
      $currentCatalogOptionsType,
      currentCo,
      currentCoId,
      currentCoType;

    coCombination.push(coId);
    this.selectCatalogOption($catalogOptionsWrapper, $catalogOptions, $selectedCatalogOptionsType, catalogId, coId);

    $otherCatalogOptionsType.each(function() {
      $currentCatalogOptionsType = $(this);
      currentCo = $(this).find(".selected");
      if (currentCo.hasClass("disabled")) {
        currentCo = $();
      }
      if (currentCo.length === 0) {
        currentCo = $(this).find(".cat-option").not(".disabled").eq(0);
      }
      currentCoId = currentCo.data("co-id");
      currentCoType = currentCo.data("co-type");
      RenderVariant.selectCatalogOption($catalogOptionsWrapper, $catalogOptions, $currentCatalogOptionsType, catalogId, currentCoId);
      coCombination.push(currentCoId);
    });

    coCombination = $.map(coCombination, function(i) {
      return parseInt(i, 10);
    }).sort(function(a, b) {
      return a - b;
    }).join(",");

    return coCombination;
  };

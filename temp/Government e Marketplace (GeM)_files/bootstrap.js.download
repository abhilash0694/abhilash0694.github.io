bjQuery(document).ready(function ($) {
  var current_variant,
    loading = '<div class="ajax-loader"></div>';

  var $localCache = {
    data: {},
    exist: function (url) {
        return $localCache.data.hasOwnProperty(url) && $localCache.data[url] !== null;
    },
    get: function (url) {
        return $localCache.data[url];
    },
    set: function (url, cachedData) {
        $localCache.data[url] = cachedData;
    }
  };

  var $qview_popup = $("<div/>").qtip({
    overwrite: true,
    content:{
      text:loading,
      title:{
        text:" ",
        button:"close"
      }
    },
    position:{
        my:'center',
        at:'center',
        target:$(window),
        viewport:$(window),
        type:'absolute'
    },
    events:{
      hide:function (event, api) {
        if(current_variant && ($('#search-results').length > 0))
        {
          $("body,html").animate({ scrollTop: (current_variant.offset().top - 90)});
        }
      }
    },
    style:{
        classes:'ui-tooltip-light popup-dialog qview-popup',
        width:1000
    },
    show:{
        modal:{
          on: true,
          effect: false
        }
    },
    hide: 'unfocus'
  });

  function qview_content(url){
    if ($localCache.exist(url)) {
      var resp = $($localCache.get(url));
      check_prev_next(resp);
      $qview_popup.qtip('api').set('content.text', resp);
    }
    else{
      if($('#quick-view').length){
        $('#quick-view').prepend(loading);
      }
      else{
        $qview_popup.qtip('api').set('content.text', loading);
      }
      $.ajax({
        url: url,
        type: 'GET',
        success:function (response) {
          var updated_response = $(response);
          check_prev_next(updated_response);
          $qview_popup.qtip('api').set('content.text', updated_response);
          $localCache.set(url, response);
        }
      });
    }
  }

  function check_prev_next(data){
    if (current_variant.prev().length===0){
      data.find('.qview_prev').addClass('disabled');
    }
    if (current_variant.next().length===0) {
      data.find('.qview_next').addClass('disabled');
    }
  }

  $(".quick_view").live('click',function () {
    current_variant = $(this).parents('li');
    var current_url = current_variant.find('a.quick_view').attr('href');
    $qview_popup.qtip('show');
    qview_content(current_url);
    return false;
  });

  $(".qview_prev").live('click',function (e) {
    var new_variant = current_variant.prev();
    if(new_variant.length===0)
      return false;
    current_variant = new_variant;
    var current_url = current_variant.find('a.quick_view').attr('href');
    qview_content(current_url);
    return false;
  });

  $(".qview_next").live('click',function () {
    var new_variant = current_variant.next();
    if(new_variant.length===0)
      return false;
    current_variant = new_variant;
    var current_url = current_variant.find('a.quick_view').attr('href');
    qview_content(current_url);
    return false;
  });

});


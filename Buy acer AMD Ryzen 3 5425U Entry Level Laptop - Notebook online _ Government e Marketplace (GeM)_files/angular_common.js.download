$(document).on('keyup', '#aadhaar_number', function(){
    if($('#aadhaar_number').val().length < 1){
        $('#show').text('');
    }
    else{
        var a = '';
        if($('#aadhaar_number').val().length <= 8){
            for(var i = 0; i < $('#aadhaar_number').val().length; i++){                
                a = a + 'X';           
                $('#show').text(a);  
            }
        }
        else
        {
            for(var i = 0; i < $('#aadhaar_number').val().length; i++){ 
                if(i <= 7){
                    a = a + 'X';           
                    $('#show').text(a);  
                }
                else{          
                    a = a + $('#aadhaar_number').val().substring(i,i+1);           
                    $('#show').text(a);  
                }
            }
        }
    }
